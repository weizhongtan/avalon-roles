{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": "error",
        "max-len": ["warn", {"ignoreComments": true, "ignoreStrings": true}],
        "no-console": ["off"]
    }
}
